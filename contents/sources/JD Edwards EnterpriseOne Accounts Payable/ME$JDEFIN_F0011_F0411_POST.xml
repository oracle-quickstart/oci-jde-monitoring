        <SQLQuery>
            <![CDATA[select 'JDE Financial sensor: Voucher Post Code Mismatch Between F0411 and F0011' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'NumRecords:' || s.NumRecords key_data FROM (SELECT COUNT(DISTINCT (CONCAT(RPICU, RPICUT))) NumRecords from CRPDTA.F0411, CRPDTA.F0011 WHERE RPICU=ICICU AND RPICUT=ICICUT   AND ((RPPOST='D' AND ICIST<>'D' AND (SELECT COUNT(*) FROM CRPDTA.F0411 WHERE RPICU=F0011.ICICU AND RPPOST<>'D')=0)   OR (RPPOST<>'D' AND ICIST='D')) AND RPUPMJ < (select (to_char ( sysdate,'YYYY')-1900)*1000 +to_char ( sysdate,'DDD') from dual) ORDER BY RPICU, RPICUT) s]]>
        </SQLQuery>
