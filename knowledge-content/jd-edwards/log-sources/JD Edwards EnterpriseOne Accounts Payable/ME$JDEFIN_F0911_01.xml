    <DisplayName>JDE Financial sensor: Number of posted vouchers with not null total amount</DisplayName>
    <Description><![CDATA[If  F0911 batch type  is V and posted code is P, then the total amount (AA) for all records must be zero]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'JDE Financial sensor: Number of posted vouchers with not null total amount' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'F0911_VOUCHER_VALUE_NOT_0:' || s.F0911_VOUCHER_VALUE_NOT_0 key_data FROM (SELECT COUNT(TOTAL_AMOUNT) F0911_VOUCHER_VALUE_NOT_0 FROM (SELECT GLICU, SUM(GLAA) AS TOTAL_AMOUNT FROM {business_data_schema}.F0911 LEFT JOIN {business_data_schema}.F0011 ON GLICU=ICICU WHERE ICPOB='N' AND TRIM(GLICUT)='V' AND TRIM(GLPOST)='P' GROUP BY GLICU ORDER BY GLICU ASC) WHERE TOTAL_AMOUNT<>0) s]]>
        </SQLQuery>
